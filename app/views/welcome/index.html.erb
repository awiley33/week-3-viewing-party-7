<% if current_user %>
    <%= link_to "Log Out", logout_path, method: :delete %><br>
    <section class='existing-users'>
        <p>Existing Users: </p>
        <% @users.each do |user| %>
            <li><%= user.email %></li>
        <% end %>
    </section>
<% else %>
    <%= link_to "Register as a User", register_path %><br>
    <%= link_to "Log In", login_path %>
<% end %>