<% if current_user %>
    <%= link_to "Log Out" %>
<% else %>
    <%= link_to "Register as a User", register_path %><br>
    <%= link_to "Log In", login_path %>
<% end %>

<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= link_to user.email, user_path(user.id) %></li>
    <% end %>
</section>